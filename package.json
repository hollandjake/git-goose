{
  "name": "git-goose",
  "version": "0.3.0",
  "description": "a mongoose plugin that enables git like change tracking",
  "license": "MIT",
  "author": "Jake Holland <contact@hollandjake.com>",
  "homepage": "https://github.com/hollandjake/git-goose",
  "repository": {
    "type": "git",
    "url": "git://github.com/hollandjake/git-goose.git"
  },
  "bugs": {
    "url": "https://github.com/hollandjake/git-goose/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "directories": {
    "lib": "./src",
    "test": "./tests"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "gts clean",
    "clean:build": "pnpm clean && pnpm build",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "mongoose": ">=8.0.0",
    "rfc6902": "^5.1.1"
  },
  "devDependencies": {
    "@mnrendra/rollup-plugin-mixexport": "^1.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.3.0",
    "@vitest/coverage-istanbul": "^2.0.5",
    "eslint": "^8.57.0",
    "gts": "^5.3.1",
    "mongodb-memory-server": "^10.0.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rollup": "^4.20.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "keywords": [
    "change tracking",
    "git",
    "history",
    "mongoose",
    "typescript"
  ],
  "readme": "./README.md"
}
